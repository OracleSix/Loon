[Rewrite]
#bing -> google
^https:\/\/cn\.bing\.com\/search\?q=g\+([^&]+)&form=(.+) 302 https://www.google.com/search?q=$1
#bing -> baidu
^https:\/\/cn\.bing\.com\/search\?q=bd\+([^&]+)&form=(.+) 302 https://m.baidu.com/s?from=$2&word=$1

